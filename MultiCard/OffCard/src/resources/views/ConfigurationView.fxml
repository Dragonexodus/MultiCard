                                    <Insets right="10.0" />
                                 </HBox.margin></Button>
                              <Separator HBox.hgrow="SOMETIMES" />
                                      <Button fx:id="resetIdentification" mnemonicParsing="false" text="zur端cksetzen" />
                           </children>
                        </HBox>
                            </children>
                        </GridPane>
                    </content>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </padding>
                </TitledPane>
